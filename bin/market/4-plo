#!/usr/bin/env bash

set -e

. $(PWD)/bin/init


export SETH_ASYNC=yes
nonce=$(seth nonce $ETH_FROM)
echo "nonce: ${nonce}"
for i in {1..3}; do
  seth send -N "$nonce" "$MEDAL" "safeMint(address,string)" "$CLAIMS_PROXY" '"Qma2jDH9gHvvmmEy5scDpJbn81rAHG9zaeQHFXB8XHiXwy"'
  nonce=$(( ${nonce} + 1 ))
done
echo "nonce: ${nonce}"
