#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

to=$ARENA_CLAIMS

export SETH_ASYNC=yes
nonce=$(seth nonce $ETH_FROM)
echo "nonce: ${nonce}"
for i in {1..16}; do
  seth send -N "$nonce" "$SEASONMEDAL" "safeMint(address,string)" "$ARENA_CLAIMS" '"Qmea3egkqPUWh5NTmapoHbMny5pvxKtWKpg8dR6eunxidN/'${i}'.json"'
  nonce=$(( ${nonce} + 1 ))
done
echo "nonce: ${nonce}"
