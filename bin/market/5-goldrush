#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

ids=(
0x2a030001030001010000000000000003000000000000000000000000000007a6
0x2a030001030001010000000000000003000000000000000000000000000003d5
0x2a03000103000101000000000000000300000000000000000000000000000659
0x2a0300010300010100000000000000030000000000000000000000000000075c
0x2a030001030001010000000000000003000000000000000000000000000005f0
0x2a030001030001010000000000000003000000000000000000000000000005b6
0x2a03000103000101000000000000000300000000000000000000000000000384
0x2a0300010300010100000000000000030000000000000000000000000000033b
0x2a03000103000101000000000000000300000000000000000000000000000449
0x2a03000103000101000000000000000300000000000000000000000000000757
0x2a030001030001010000000000000003000000000000000000000000000001e2
0x2a0300010300010100000000000000030000000000000000000000000000044a
0x2a030001030001010000000000000003000000000000000000000000000006e3
  )

to=$CLAIMS_PROXY
for id in "${ids[@]}";do
  echo "sending land [$id] to [$to]"
  evo mkt-gen claim "${id?}" --chain "${SETH_CHAIN?}"
  echo "claimed $id"
  evo cmn-tkn transferFrom "${ETH_FROM?}" "${to?}" "${id?}" --chain "${SETH_CHAIN?}"
  echo "sent land [$id] to [$to]"
done

# https://www.evolution.land/land/3/land/detail?tokenId=2a03000103000101000000000000000300000000000000000000000000000096

