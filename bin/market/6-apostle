#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

ids=(
0x2a03000103000102000000000000000300000000000000000000000000001183
0x2a030001030001020000000000000003000000000000000000000000000010fd
0x2a03000103000102000000000000000300000000000000000000000000001100
0x2a03000103000102000000000000000300000000000000000000000000001103
0x2a0300010300010200000000000000030000000000000000000000000000110e
0x2a03000103000102000000000000000300000000000000000000000000001123
0x2a03000103000102000000000000000300000000000000000000000000001182
0x2a03000103000102000000000000000300000000000000000000000000001126
0x2a03000103000102000000000000000300000000000000000000000000001133
0x2a03000103000102000000000000000300000000000000000000000000001125
0x2a03000103000102000000000000000300000000000000000000000000001135
0x2a03000103000102000000000000000300000000000000000000000000001147
0x2a03000103000102000000000000000300000000000000000000000000001148
0x2a03000103000102000000000000000300000000000000000000000000001180
0x2a0300010300010200000000000000030000000000000000000000000000117f
0x2a0300010300010200000000000000030000000000000000000000000000117e
0x2a0300010300010200000000000000030000000000000000000000000000117d
0x2a0300010300010200000000000000030000000000000000000000000000117c
0x2a03000103000102000000000000000300000000000000000000000000001174
0x2a0300010300010200000000000000030000000000000000000000000000117a
0x2a03000103000102000000000000000300000000000000000000000000001173
0x2a03000103000102000000000000000300000000000000000000000000001179
0x2a03000103000102000000000000000300000000000000000000000000001178
0x2a03000103000102000000000000000300000000000000000000000000001175
  )

to=$CLAIMS_PROXY
for id in "${ids[@]}";do
  echo "sending apostle [$id] to [$to]"
  seth send "$GEN0APOSTLE_PROXY" "claimERC721Tokens(uint256)" "${id?}" --chain "${SETH_CHAIN?}"
  echo "claimed $id"
  evo cmn-tkn transferFrom "${ETH_FROM?}" "${to?}" "${id?}" --chain "${SETH_CHAIN?}"
  echo "sent apostle [$id] to [$to]"
done

# https://www.evolution.land/land/3/land/detail?tokenId=2a03000103000101000000000000000300000000000000000000000000000096

