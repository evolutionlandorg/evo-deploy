#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

ids=(
0x2a0300010300010200000000000000030000000000000000000000000000117b
0x2a03000103000102000000000000000300000000000000000000000000001181
0x2a03000103000102000000000000000300000000000000000000000000001177
  )

to=$CLAIMS_PROXY
for id in "${ids[@]}";do
  echo "sending land [$id] to [$to]"
  seth send "$GEN0APOSTLE_PROXY" "claimERC721Tokens(uint256)" "${id?}" --chain "${SETH_CHAIN?}"
  echo "claimed $id"
  evo cmn-tkn transferFrom "${ETH_FROM?}" "${to?}" "${id?}" --chain "${SETH_CHAIN?}"
  echo "sent land [$id] to [$to]"
done

# https://www.evolution.land/land/3/land/detail?tokenId=2a03000103000101000000000000000300000000000000000000000000000096

