#!/usr/bin/env bash

set -e

. $(PWD)/bin/init

ids=(
19002440208967408626316923705014494878449749569831862348137923003784877838710
19002440208967408626316923705014494878449749569831862348137923003784877838699
19002440208967408626316923705014494878449749569831862348137923003784877838638
19002440208967408626316923705014494878449749569831862348137923003784877838684
  )

to=$CLAIMS_PROXY
for id in "${ids[@]}";do
  echo "sending apostle [$id] to [$to]"
  seth send "$GEN0APOSTLE_PROXY" "claimERC721Tokens(uint256)" "${id?}" --chain "${SETH_CHAIN?}"
  echo "claimed $id"
  evo cmn-tkn transferFrom "${ETH_FROM?}" "${to?}" "${id?}" --chain "${SETH_CHAIN?}"
  echo "sent apostle [$id] to [$to]"
done

# https://www.evolution.land/land/3/land/detail?tokenId=2a03000103000101000000000000000300000000000000000000000000000096

