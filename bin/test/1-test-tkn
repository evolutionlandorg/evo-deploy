tokens=("GOLD" "WOOD" "WATER" "FIRE" "SOIL")
# tokens=("RING" "KTON")
to=$mess
for token in "${tokens[@]}"; do
  seth send "$(eval echo "\$TOKEN_ERC20_${token}")" "mint(address,uint256)" "$to" "$amount"
done

